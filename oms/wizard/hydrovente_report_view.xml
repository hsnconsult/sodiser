<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="oms_sortiedouane_report_view" model="ir.ui.view">
        <field name="name">Sortie Douane</field>
        <field name="model">oms.sortiedouane.report</field>
        <field name="arch" type="xml">
        <form string="Sorties  Douane">
            <group col="4">
                <field name="decade"/>
                <field name="produit"/>
                <field name="date_cour" invisible="1"/>
            </group>
            <footer>
                <button name="check_report" string="Imprimer" type="object" default_focus="1" class="oe_highlight"/>
                or 
                <button string="Annuler" class="oe_link" special="cancel" />
            </footer>
        </form>
        </field>
    </record>

    <record id="action_oms_sortiedouane_report" model="ir.actions.act_window">
        <field name="name">Sortie Douane</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">oms.sortiedouane.report</field>
        <field name="view_type">form</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="oms_sortiedouane_report_view"/>
        <field name="target">new</field>
    </record>

    <record id="action_oms_sortiedouane_values" model="ir.values">
        <field name="model_id" ref="oms.model_oms_commande" />
        <field name="name">Sortie Douane</field>
        <field name="key2">client_print_multi</field>
        <field name="value" eval="'ir.actions.act_window,' +str(ref('action_oms_sortiedouane_report'))" />
        <field name="key">action</field>
        <field name="model">oms.sortiedouane.report</field>
    </record>

